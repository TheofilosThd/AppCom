<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_tel"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.teo.appcom.TelActivity">

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/n3"
        android:layout_marginLeft="24dp"
        android:layout_marginStart="24dp"
        android:id="@+id/n3_btn"
        android:layout_alignTop="@+id/n2_btn"
        android:layout_toRightOf="@+id/n2_btn"
        android:layout_toEndOf="@+id/n2_btn" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/n6"
        android:layout_alignBottom="@+id/n5_btn"
        android:layout_alignRight="@+id/n3_btn"
        android:layout_alignEnd="@+id/n3_btn"
        android:id="@+id/n6_btn" />

    <TextView
        android:background="@color/white"
        android:layout_width="350dp"
        android:layout_height="90dp"
        android:layout_marginTop="12dp"
        android:id="@+id/telephone_input"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/n8"
        android:id="@+id/n8_btn"
        android:layout_below="@+id/n5_btn"
        android:layout_alignLeft="@+id/n5_btn"
        android:layout_alignStart="@+id/n5_btn"
        android:layout_marginTop="11dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/n7"
        android:id="@+id/n7_btn"
        android:layout_alignTop="@+id/n8_btn"
        android:layout_alignLeft="@+id/n4_btn"
        android:layout_alignStart="@+id/n4_btn" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/n5"
        android:id="@+id/n5_btn"
        android:layout_below="@+id/n2_btn"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="14dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/n4"
        android:id="@+id/n4_btn"
        android:layout_alignTop="@+id/n5_btn"
        android:layout_alignLeft="@+id/n1_btn"
        android:layout_alignStart="@+id/n1_btn" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/n9"
        android:id="@+id/n9_btn"
        android:layout_alignTop="@+id/n8_btn"
        android:layout_alignLeft="@+id/n6_btn"
        android:layout_alignStart="@+id/n6_btn" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/n1"
        android:layout_marginTop="38dp"
        android:id="@+id/n1_btn"
        android:layout_below="@+id/telephone_input"
        android:layout_toLeftOf="@+id/n2_btn"
        android:layout_toStartOf="@+id/n2_btn"
        android:layout_marginRight="26dp"
        android:layout_marginEnd="26dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/n2"
        android:id="@+id/n2_btn"
        android:layout_alignTop="@+id/n1_btn"
        android:layout_alignLeft="@+id/n5_btn"
        android:layout_alignStart="@+id/n5_btn" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/n"
        android:id="@+id/n0_btn"
        android:layout_below="@+id/n8_btn"
        android:layout_alignLeft="@+id/n8_btn"
        android:layout_alignStart="@+id/n8_btn"
        android:layout_marginTop="13dp" />

    <Button
        android:text="CALL"
        android:background="#00ff00"
        android:textSize="45dp"
        android:layout_width="110dp"
        android:layout_height="60dp"
        android:id="@+id/call_btn"
        android:layout_alignBottom="@+id/n0_btn"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:text="DEL"
        android:background="#ff0000"
        android:textSize="45dp"
        android:layout_width="110dp"
        android:layout_height="60dp"
        android:id="@+id/delete_btn"
        android:layout_alignBottom="@+id/n0_btn"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

</RelativeLayout>
